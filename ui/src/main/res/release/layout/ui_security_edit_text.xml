<?xml version="1.0" encoding="utf-8"?>
<com.aaron.ui.widget.PwdInputView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ui_pwd_input"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:layout_marginLeft="64dp"
    android:layout_marginRight="64dp"
    android:layout_marginTop="32dp"
    android:layout_gravity="center"
    android:inputType="numberPassword|none"
    android:background="@null"
    app:pwv_maxLength="6"
    app:pwv_borderColor="#66000000"
    app:pwv_pwdColor="@color/base_black"
    app:pwv_strokeWidth="0.4dp"
    app:pwv_radius="0dp"
    tools:ignore="PxUsage" />